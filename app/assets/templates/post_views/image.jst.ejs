<div class="col-md-2">
	<!-- User Profile image goes here -->
	<img src="http://i.imgur.com/cFdN7IR.jpg" class="img-circle profile-image">
</div>
			
<div class="col-md-10 feed-post">
	<div class="left-triangle"></div>
	<div class="post-background">
		<div class="post-author"><%= post.get('author').username %></div>
		<% post.pictures().forEach(function(picture){ %>
			<div class="image-post-frame"> 
				<img  class="image-post-picture" src="<%= picture.get('large_url') %>" />
			</div>
		<% }) %>
		<h4><%= post.escape('title') %></h4>
		<div class="post-md-body"><%= post.escape('text') %></div>
	</div>
<div class="clear-div"></div>
</div>

